#include "main.h"
/**
* times_table - print times table
* Return : void
*/
void times_table(void)
{
int a = 0;
int b;
int multi;
while (a <= 9)
{
b = 0;
while (b <= 9)
{
multi = a * b;
if (b = 0)
{
_putchar('0' + multi);
}
else if (multi < 10)
{
_putchar(' ');
_putchar('0' + multi);
}
else
{
_putchar('0' + multi / 10);
_putchar('0' + multi % 10);
}
if (b < 9)
{
_putchar(',');
_putchar(' ');
}
b++;
}
_putchar('\n');
a++;
}
}
